        "fast-glob": "^3.2.2",
        "magic-string": "^0.25.7",
        "postcss": "^7.0.27",
        "postcss-selector-parser": "^6.0.2",
        "rollup": "^2.0.2",
        "rollup-plugin-terser": "^5.2.0",
        "rollup-pluginutils": "^2.8.2",
        "systemjs": "^6.2.5"
    },
    "devDependencies": {
        "@types/cheerio": "^0.22.16",
        "@types/cssnano": "^4.0.0",
        "@types/node": "^13.9.0",
        "@types/react": "^16.9.23",
